<?xml version="1.0" encoding="utf-8" ?>
<Defs>

<ThingDef ParentName="BaseFilth">
    <defName>DV_MuckPool</defName>
   <label>muck pool</label>
   <statBases>
     <Beauty>-12</Beauty>
     <Cleanliness>-10</Cleanliness>
   </statBases>
    <passability>PassThroughOnly</passability>
   <pathCost>30</pathCost>
   <graphicData>
      <texPath>Things/Filth/PoolSoft</texPath>
      <color>(74, 75, 54, 180)</color>
      <drawSize>2</drawSize>
   </graphicData>
   <filth>
     <ignoreFilthMultiplierStat>true</ignoreFilthMultiplierStat>
     <rainWashes>true</rainWashes>

     <disappearsInDays>12~30</disappearsInDays>
     <cleaningWorkToReduceThickness>70</cleaningWorkToReduceThickness>
     <cleaningSound>Interact_CleanFilth_Fluid</cleaningSound>
     <canFilthAttach>true</canFilthAttach>
     <maxThickness>1</maxThickness>
   </filth>
 </ThingDef>

</Defs>